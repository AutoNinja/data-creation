  <script type="text/javascript" src="/js/vendor/nav.js"></script>

  <!-- page scripts -->
  <script type="text/javascript" src="/js/index.js"></script>
  <script>
    $("#date").text("EST "+new Date().toLocaleDateString("en-US",{
      weekday: 'long',
      year: 'numeric',
      month: 'long',
