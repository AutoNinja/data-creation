<!doctype html>
<html lang="en">
<head>

  <!-- base stylesheets -->
  <%- include ../partials/header %>

  <!-- custom stylesheets -->
  <link type="text/css" rel="stylesheet" href="css/page_newdata.css" />


</head>
<body>

  <% if (user==="manual") { %>
    <%- include ../partials/nav %>
    <%- include ../partials/headerbar %>
  <% } else { %>
    <%- include ../partials/nav_auto %>
    <%- include ../partials/headerbar_auto %>
  <% } %>

  <div id="content">
    <input type="hidden" id="reqTypeValue" value="R"></input>


    <section class="main-content">
      <div id="jsGrid"></div>

      <div id="detailsDialog" hidden>

        <div class="dialog-content" id="dialog-textboxes">
          <form id='detailsForm'>
            <button class='dialog-btn' type='submit' id='submit'>Add</button>
            <button class='dialog-btn' type='button' id='cancel' onClick='$("#detailsDialog").dialog("close")'>Cancel</button>
          </form>

        </div>

      </div>

      <div class="table-buttons-wrapper">
        <div class="row">
          <div class="col-sm-12 col-md-12 col-xs-12 text-center">
            <input type="button" class="home table-btn" id="home" value="Home">
            <input type="button" class="save table-btn" id="save" value="Submit">
          </div>
        </div>
      </div>
    </section>

  </div>

  <script>
    var user = <%- JSON.stringify(user) %>;
  </script>
  <!-- Vendor Javascript -->
  <%- include ../partials/scripts %>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jsgrid/1.5.3/jsgrid.min.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.16.0/jquery.validate.min.js"></script>
  <script type="text/javascript" src="http://code.jquery.com/ui/1.11.1/jquery-ui.min.js"></script>
  <!-- custom Javascript -->
  <script type="text/javascript" src='/js/page_newdata.js'></script>

</body>
</html>
