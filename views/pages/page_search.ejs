<!doctype html>
<html lang="en">
<head>
  <!-- base stylesheets -->
  <%- include ../partials/header %>


</head>
<body>

  <% if (user==="manual") { %>
    <%- include ../partials/nav %>
    <%- include ../partials/headerbar %>
  <% } else { %>
    <%- include ../partials/nav_auto %>
    <%- include ../partials/headerbar_auto %>
  <% } %>

  <div id="content">
    <input type="hidden" id="reqTypeValue" value="R"></input>



    <section class="main-content">
      <div id="jsGrid"></div>

      <div class="table-buttons-wrapper">
        <div class="row">
          <div class="col-sm-12 col-md-12 col-xs-12 text-center">
            <input type="button" class="home table-btn" id="home" value="Home">
          </div>
        </div>
      </div>
    </section>

  </div>

  <script>
    var user = <%- JSON.stringify(user) %>;
  </script>
  <!-- Vendor Javascript -->
  <%- include ../partials/scripts %>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jsgrid/1.5.3/jsgrid.min.js"></script>

  <!-- custom Javascript -->
  <script type="text/javascript" src='/js/page_search.js'></script>

</body>
</html>
